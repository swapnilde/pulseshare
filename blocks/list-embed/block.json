{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"name": "pulseshare/list-embed",
	"version": "1.0.0",
	"title": "Podcast & Episodes",
	"category": "pulseshare",
	"description": "Display a list of episodes to embed in your posts.",
	"keywords": [ "spotify", "podcast", "show", "embed" ],
	"supports": {
		"html": false
	},
	"attributes": {
		"blockID": {
			"type": "string",
			"default": ""
		},
		"episodesArray": {
			"type": "array",
			"default": []
		},
		"displayType": {
			"type": "string",
			"default": "full"
		},
		"currentEpisode": {
			"type": "object",
			"default": {}
		},
		"isVideo": {
			"type": "boolean",
			"default": false
		},
		"height": {
			"type": "string"
		},
		"width": {
			"type": "string"
		}
	},
	"textdomain": "pulseshare",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js"
}
